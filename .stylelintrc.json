{
  "extends": ["stylelint-config-standard"],
  "rules": {
    "font-family-no-missing-generic-family-keyword": null,
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["font-face"]
      }
    ],
    "no-descending-specificity": null,
    "selector-pseudo-class-no-unknown": [
      true,
      {
        "ignorePseudoClasses": ["any-link"]
      }
    ]
  }
}
